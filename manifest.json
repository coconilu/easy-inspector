{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "author": "bayeswang",
  "permissions": [
    "activeTab",
    "storage",
    "webRequestBlocking",
    "webRequest",
    "<all_urls>"
  ],
  "options_page": "index.html",
  "background": {
    "scripts": ["dist/interceptor.js"]
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "manifest_version": 2
}
